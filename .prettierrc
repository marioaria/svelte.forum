{
  "$schema": "https://json.schemastore.org/prettierrc",
  "printWidth": 120,
  "singleQuote": true,
  "trailingComma": "none",
  "plugins": ["prettier-plugin-svelte", "prettier-plugin-tailwindcss"],
  "overrides": [
    {
      "files": "*.svelte",
      "options": {
        "parser": "svelte"
      }
    }
  ],
  "tailwindStylesheet": "./src/lib/assets/main.css"
}
