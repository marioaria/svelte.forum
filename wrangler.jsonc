{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "svelte-forum",
  "main": ".svelte-kit/cloudflare/_worker.js",
  "compatibility_date": "2026-01-28",
  "compatibility_flags": ["nodejs_compat_v2"],
  "assets": {
    "binding": "ASSETS",
    "directory": ".svelte-kit/cloudflare"
  },
  "observability": {
    "enabled": true
  }
}
